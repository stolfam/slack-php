{
  "name": "ataccama/slack-php",
  "license": "MIT",
  "description": "Easy-to-use PHP classes for Slack integration",
  "version": "4.0.0",
  "type": "library",
  "require": {
    "php": ">= 8.0.0",
    "nette/utils": ">= 3.0",
    "php-curl-class/php-curl-class": "^8.6",
    "ataccama/utils-php": "^4.0",
    "tracy/tracy": "^2.6"
  },
  "require-dev": {
    "nette/tester": ">= 2.3"
  },
  "autoload": {
    "classmap": ["src/"],
    "files": ["src/bootstrap.php"]
  },
  "minimum-stability": "stable",
  "config": {
    "platform": {
      "php": "8.0"
    }
  }
}
